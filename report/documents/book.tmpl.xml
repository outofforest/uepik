<table:table table:name="PiK" table:style-name="taLandscape">
    <office:forms form:automatic-focus="false" form:apply-design-mode="false"/>
    <table:table-column table:style-name="co1" table:default-cell-style-name="ce67"/>
    <table:table-column table:style-name="co2" table:default-cell-style-name="ce71"/>
    <table:table-column table:style-name="co3" table:default-cell-style-name="ce77"/>
    <table:table-column table:style-name="co6" table:default-cell-style-name="ce90"/>
    <table:table-column table:style-name="co7" table:number-columns-repeated="3" table:default-cell-style-name="ce94"/>
    <table:table-header-rows>
        <table:table-row table:style-name="ro1">
            <table:table-cell table:style-name="ce3" office:value-type="string" calcext:value-type="string" table:number-columns-spanned="7" table:number-rows-spanned="1">
                <text:p>ZESTAWIENIE PRZYCHODÓW I KOSZTÓW</text:p>
            </table:table-cell>
        </table:table-row>
        <table:table-row table:style-name="ro2">
            <table:table-cell table:style-name="Default" table:number-columns-repeated="7"/>
        </table:table-row>
        <table:table-row table:style-name="ro3">
            <table:table-cell table:style-name="ce6" office:value-type="string" calcext:value-type="string" table:number-columns-spanned="7" table:number-rows-spanned="1">
                <text:p>Nazwa podatnika: {{ .CompanyName }}</text:p>
            </table:table-cell>
        </table:table-row>
        <table:table-row table:style-name="ro4">
            <table:table-cell table:style-name="Default" table:number-columns-repeated="7"/>
        </table:table-row>
        <table:table-row table:style-name="ro5">
            <table:table-cell table:style-name="ce8" office:value-type="string" calcext:value-type="string">
                <text:p>Lp.</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce14" office:value-type="string" calcext:value-type="string">
                <text:p>Data</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce20" office:value-type="string" calcext:value-type="string">
                <text:p>Nr dowodu księgowego</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce14" office:value-type="string" calcext:value-type="string">
                <text:p>Opis zdarzenia</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce14" office:value-type="string" calcext:value-type="string">
                <text:p>Przychody</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce70" office:value-type="string" calcext:value-type="string">
                <text:p>KUP</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce70" office:value-type="string" calcext:value-type="string">
                <text:p>NKUP</text:p>
            </table:table-cell>
        </table:table-row>
        <table:table-row table:style-name="ro7">
            <table:table-cell table:style-name="ce66" office:value-type="float" office:value="1" calcext:value-type="float">
                <text:p>1</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce66" office:value-type="float" office:value="2" calcext:value-type="float">
                <text:p>2</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce76" office:value-type="float" office:value="3" calcext:value-type="float">
                <text:p>3</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce89" office:value-type="float" office:value="4" calcext:value-type="float">
                <text:p>4</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce66" office:value-type="float" office:value="5" calcext:value-type="float">
                <text:p>5</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce66" office:value-type="float" office:value="6" calcext:value-type="float">
                <text:p>6</text:p>
            </table:table-cell>
            <table:table-cell table:style-name="ce66" office:value-type="float" office:value="7" calcext:value-type="float">
                <text:p>7</text:p>
            </table:table-cell>
        </table:table-row>
    </table:table-header-rows>
{{ range .Months }}
    <table:table-row table:style-name="ro3">
        <table:table-cell table:style-name="ce6" office:value-type="string" calcext:value-type="string" table:number-columns-spanned="7" table:number-rows-spanned="1">
            <text:p>{{ .Month }} {{ .Year }}</text:p>
        </table:table-cell>
    </table:table-row>
{{ range .Records }}
    <table:table-row table:style-name="ro8">
        <table:table-cell office:value-type="float" office:value="{{ .Index }}" calcext:value-type="float" />
        <table:table-cell office:value-type="float" office:value="{{ .DayOfMonth }}" calcext:value-type="float" />
        <table:table-cell office:value-type="string" calcext:value-type="string">
{{ if eq .Document.SheetName "" }}
            <text:p>{{ .Document.ID }}</text:p>
{{ else }}
            <text:p><text:a xlink:href="#{{ .Document.SheetName }}" xlink:type="simple">{{ .Document.ID }}</text:a></text:p>
{{ end }}
        </table:table-cell>
        <table:table-cell office:value-type="string" calcext:value-type="string">
            <text:p>{{ .Notes }}</text:p>
        </table:table-cell>
{{ if notZero .Income.Amount }}
        <table:table-cell office:value-type="float" office:value="{{ .Income.Amount }}" calcext:value-type="float" />
{{ else }}
        <table:table-cell />
{{ end }}
{{ if notZero .CostTaxed.Amount }}
        <table:table-cell office:value-type="float" office:value="{{ .CostTaxed.Amount }}" calcext:value-type="float" />
{{ else }}
        <table:table-cell />
{{ end }}
{{ if notZero .CostNotTaxed.Amount }}
        <table:table-cell office:value-type="float" office:value="{{ .CostNotTaxed.Amount }}" calcext:value-type="float" />
{{ else }}
        <table:table-cell />
{{ end }}
    </table:table-row>
{{ end }}
    <table:table-row table:style-name="ro9">
        <table:table-cell table:style-name="ce29" office:value-type="string" calcext:value-type="string" table:number-columns-spanned="4" table:number-rows-spanned="1">
            <text:p>Razem w okresie sprawozdawczym:</text:p>
        </table:table-cell>
        <table:covered-table-cell table:number-columns-repeated="3"/>
        <table:table-cell table:style-name="ce97" office:value-type="float" office:value="{{ .MonthSummary.Income.Amount }}" calcext:value-type="float" />
        <table:table-cell table:style-name="ce97" office:value-type="float" office:value="{{ .MonthSummary.CostTaxed.Amount }}" calcext:value-type="float" />
        <table:table-cell table:style-name="ce97" office:value-type="float" office:value="{{ .MonthSummary.CostNotTaxed.Amount }}" calcext:value-type="float" />
    </table:table-row>
    <table:table-row table:style-name="ro9">
        <table:table-cell table:style-name="ce29" office:value-type="string" calcext:value-type="string" table:number-columns-spanned="4" table:number-rows-spanned="1">
            <text:p>Razem na koniec poprzedniego okresu sprawozdawczego:</text:p>
        </table:table-cell>
        <table:covered-table-cell table:number-columns-repeated="3"/>
        <table:table-cell table:style-name="ce96" office:value-type="float" office:value="{{ .AccumulatedPreviousSummary.Income.Amount }}" calcext:value-type="float" />
        <table:table-cell table:style-name="ce96" office:value-type="float" office:value="{{ .AccumulatedPreviousSummary.CostTaxed.Amount }}" calcext:value-type="float" />
        <table:table-cell table:style-name="ce96" office:value-type="float" office:value="{{ .AccumulatedPreviousSummary.CostNotTaxed.Amount }}" calcext:value-type="float" />
    </table:table-row>
    <table:table-row table:style-name="ro9">
        <table:table-cell table:style-name="ce29" office:value-type="string" calcext:value-type="string" table:number-columns-spanned="4" table:number-rows-spanned="1">
            <text:p>Razem od początku roku:</text:p>
        </table:table-cell>
        <table:covered-table-cell table:number-columns-repeated="3"/>
        <table:table-cell table:style-name="ce98" office:value-type="float" office:value="{{ .AccumulatedCurrentSummary.Income.Amount }}" calcext:value-type="float" />
        <table:table-cell table:style-name="ce98" office:value-type="float" office:value="{{ .AccumulatedCurrentSummary.CostTaxed.Amount }}" calcext:value-type="float" />
        <table:table-cell table:style-name="ce98" office:value-type="float" office:value="{{ .AccumulatedCurrentSummary.CostNotTaxed.Amount }}" calcext:value-type="float" />
    </table:table-row>
{{ end }}
</table:table>
